#!/bin/bash -l
#SBATCH -N 1
#SBATCH --tasks-per-node=28
#SBATCH --time=1:00:00
#SBATCH --job-name="CrCH3+"
#SBATCH --error="CrCH3+.e%j"
#SBATCH --output="CrCH3+.o%j"
#SBATCH --mem=120G


source ${HOME}/.bashrc
export MKL_THREADING_LAYER=""
conda activate p4env


psi4 -n 28 -i "CrCH3+.in" -o "CrCH3+.out"
