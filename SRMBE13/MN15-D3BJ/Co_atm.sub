#!/bin/bash -l
#SBATCH -N 1
#SBATCH --tasks-per-node=28
#SBATCH --time=1:00:00
#SBATCH --job-name="Co_atm"
#SBATCH --error="Co_atm.e%j"
#SBATCH --output="Co_atm.o%j"
#SBATCH --mem=120G


source ${HOME}/.bashrc
export MKL_THREADING_LAYER=""
conda activate p4env


psi4 -n 28 -i "Co_atm.in" -o "Co_atm.out"
